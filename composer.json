{
    "name": "maunigit/php-car",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/maunigit/php-car.git"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "appium/php-client": "dev-master"
    },
    "require-dev" : {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
		"psr-4": {
			"Application\\": "src/Application/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Application\\": "tests/Application/"
		}
    },
    "scripts": {
		"test": ".\\vendor\\bin\\phpunit"
    }
}